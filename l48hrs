SELECT [MRN]
	  ,[VISIT]
      ,[EVENT_CD_DISPLAY]
      ,[EVENT_SET_KEY_NAME]
      ,[EVENT_END_DT_TM]
      ,[EVENT_TAG]
      ,[PERFORMED_DT_TM]
      ,[RESULT_STATUS_DESCRIPTION]
      ,[RESULT_VAL]
      ,[ORDER_MNEMONIC]
      ,([NAME_FULL_FORMATTED])ORDERED_BY
  FROM [CDM].[dbo].[CDM_LAB]
  WHERE [EVENT_CD]='3221230977'
	AND [EVENT_END_DT_TM]>=DATEADD(HOUR,-48,GETDATE());
