SELECT VISITS.VISIT
	
	
	
	FROM(SELECT V.VISIT  /*PULL FIN, UNIT, ROOM, AND BED*/
	,V.[LOC_NURSE_UNIT]
	,V.[LOC_ROOM]
	,V.[LOC_ROOM]
	FROM [CDM].[dbo].[CDM_VISIT] AS  V
	 LEFT JOIN [CDM].[dbo].[CDM_ENCOUNTER] AS E ON V.VISIT = E.VISIT AND HOSP_ASSOC = 'VM'
	where V.DISCH_DT >= '20200101' /*DC AFTER 01-01-20*/
	and PAT_TYPE IN ('IPMED','IPSUR','OBM','OBS','OPCB')) VISITS
